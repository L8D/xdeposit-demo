{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "clean": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", "deployments/**", "artifacts/**", "typechain-types/**", "cache/**"]
    },
    "test": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "deploy-contracts": {
      "cache": false,
      "dependsOn": ["build", "^deploy-contracts"]
    },
    "post-deploy-contracts": {
      "cache": false
    }
  }
}
