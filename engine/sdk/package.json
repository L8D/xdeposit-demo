    "bs58": "^5.0.0",
    "eip1193-provider": "^1.0.1",
    "ethers": "^6.0.8",
    "loglevel": "^1.8.1",
    "not-a-log": "^1.0.1",
    "source-map-support": "^0.5.21",
    "treeify": "^1.1.0",
