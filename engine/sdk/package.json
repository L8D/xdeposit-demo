  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "clean": "rimraf build",
    "build": "tsc",
    "lint": "eslint src",
    "test": "ava",
    "coverage": "c8 ava",
    "build:docs": " markdown-toc README.md -i && typedoc --plugin typedoc-plugin-markdown  --excludePrivate --out docs src/index.ts && rimraf docs/README.md",
    "build:json-schema": "ts-node src/private/generate-json-schema.ts",
    "dev:json-schema": "nodemon  --delay 500ms --watch 'build/model' --exec pnpm run build:json-schema",
    "dev:test": "SKIP_E2E=true ava -w",
    "typedoc:watch": "typedoc --plugin typedoc-plugin-markdown --entryDocument docs/README.md --excludePrivate --out docs src/index.ts --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
